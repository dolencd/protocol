{
  "name": "lap-protocol",
  "version": "0.1.1",
  "description": "The Low bandwidth Application Protocol simplifies the communication of applications connected by low bandwidth links.",
  "keywords": [
    "IoT",
    "Low Bandwidth",
    "Communication Protocol"
  ],
  "scripts": {
    "test": "yarn jest --detectOpenHandles",
    "docs": "typedoc ./src/index.ts",
    "typecheck": "yarn tsc --project ./tsconfig-typecheck.json",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{ts,tsx}'",
    "build": "./scripts/build.sh",
    "prepublishOnly": "yarn build"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist/",
    "protocol.proto"
  ],
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-formatter-pretty": "^3.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^26.0.1",
    "jsdoc": "^3.6.6",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.3",
    "typescript-eslint-parser": "^22.0.0"
  },
  "dependencies": {
    "deep-object-diff": "^1.1.0",
    "lodash": "^4.17.21",
    "protobufjs": "^6.10.2"
  }
}
